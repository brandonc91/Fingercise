import java.util.ArrayList;
import java.util.HashMap;

public class Player {
	private String name;
	private ArrayList<Integer> scores;
	public Player(String name) 
	{
		this.name = name;
		scores = new ArrayList<Integer>();
	}
	
	public String getName() {
		return name;
	}
	
	public void newScore(int score) {
		scores.add(score);
	}
	
}
